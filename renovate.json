{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":disableDependencyDashboard"
  ],
  "timezone": "Europe/Paris",
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "golang.org/x"
      ],
      "schedule": [
        "every month"
      ]
    }
  ],
  "postUpdateOptions": [
    "gomodTidy"
  ]
}
